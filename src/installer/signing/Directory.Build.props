<Project>

  <Import Project="$([MSBuild]::GetPathOfFileAbove(Directory.Build.props, $(MSBuildThisFileDirectory)..))" />

  <PropertyGroup>
    <TargetFramework>$(NetCoreAppCurrent)</TargetFramework>

    <!-- TODO: (Consolidation) Skip signing steps by default for non-official builds (or official builds with post-build signing enabled). https://github.com/dotnet/runtime/issues/1026 -->
    <SkipSigning>$(PostBuildSign)</SkipSigning>
    <SkipSigning Condition="'$(SkipSigning)' == '' and '$(DotNetSignType)' == ''">true</SkipSigning>
  </PropertyGroup>
</Project>
